<?xml version="1.0" encoding="UTF-8"?>
<module rename-to="circuitjs1">
    <inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.http.HTTP" />
    <inherits name="com.google.gwt.json.JSON" />
    <inherits name='com.google.gwt.user.theme.clean.Clean'/>
    <inherits name='com.google.gwt.typedarrays.TypedArrays'/>
    
    <entry-point class="com.lushprojects.circuitjs1.client.circuitjs1" />
    <stylesheet src='style.css' />
    
    <!-- Enable JsInterop -->
    <set-configuration-property name="jsinterop.checks" value="DISABLED"/>

    <!-- Source paths - only declare 'client' once, subdirectories are included automatically -->
    <source path="client"/>

</module>